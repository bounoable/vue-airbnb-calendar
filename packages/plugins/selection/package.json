{
  "name": "vue-airbnb-calendar-plugin-selection",
  "version": "0.6.3",
  "description": "Selection / Reservation / Booking plugin for vue-airbnb-calendar",
  "keywords": [
    "airbnb",
    "calendar",
    "datepicker",
    "selection",
    "booking",
    "reservation",
    "vue",
    "vue-airbnb-calendar"
  ],
  "author": "Saman Hosseini <bounoable@gmail.com>",
  "homepage": "https://github.com/bounoable/vue-airbnb-calendar",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/vue-airbnb-calendar-plugin-selection.umd.min.js",
  "jsdelivr": "dist/vue-airbnb-calendar-plugin-selection.min.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bounoable/vue-airbnb-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/bounoable/vue-airbnb-calendar/issues"
  },
  "scripts": {
    "build": "rimraf ./dist && yarn build:module && yarn build:bundles && yarn build:css",
    "build:module": "tsc && mv ./dist/index.js ./dist/index.esm.js && mv ./dist/index.js.map ./dist/index.esm.js.map",
    "build:bundles": "bili",
    "build:css": "bili -c css.bili.config.ts && rm ./dist/style.js"
  },
  "dependencies": {
    "@vue/composition-api": "^0.3.2",
    "vue": "^2.6.10",
    "vue-airbnb-calendar": "^0.6.3"
  }
}
