{
  "name": "vue-airbnb-calendar-plugin-selection",
  "version": "0.11.0",
  "description": "Selection / Reservation / Booking plugin for vue-airbnb-calendar",
  "keywords": [
    "airbnb",
    "calendar",
    "datepicker",
    "selection",
    "booking",
    "reservation",
    "vue",
    "vue-airbnb-calendar"
  ],
  "author": "Saman Hosseini <bounoable@gmail.com>",
  "homepage": "https://github.com/bounoable/vue-airbnb-calendar",
  "license": "MIT",
  "main": "dist/index.js",
  "unpkg": "dist/vue-airbnb-calendar-plugin-selection.umd.min.js",
  "jsdelivr": "dist/vue-airbnb-calendar-plugin-selection.min.umd.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bounoable/vue-airbnb-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/bounoable/vue-airbnb-calendar/issues"
  },
  "scripts": {
    "dev": "vue-cli-service serve dev/main.ts",
    "build": "rimraf ./dist && bili && yarn build:module && yarn build:css",
    "build:module": "rimraf ./lib && tsc --p module.tsconfig.json",
    "build:css": "bili -c css.bili.config.ts && rm ./dist/style.js"
  },
  "dependencies": {
    "@vue/composition-api": "^0.6.6",
    "date-fns": "^2.8.1",
    "vue": "^2.6.10",
    "vue-airbnb-calendar": "^0.11.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.12.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "bili": "^4.8.1",
    "core-js": "^3.3.2",
    "cssnano": "^4.1.10",
    "lerna": "^3.18.3",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^8.0.0",
    "tailwindcss": "^1.1.3",
    "typescript": "^3.6.4",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
